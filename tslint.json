{
    "defaultSeverity": "error",
    "extends": [
        "tslint-react"
    ],
    "rules": {
        "member-access": false,
        "ordered-imports": true,
        "quotemark": false,
        "no-console": false,
        "semicolon": false,
        "jsx-no-lambda": false,
        "jsx-no-multiline-js": false
    },
    "linterOptions": {
        "exclude": [
            "config/**/*.js",
            "node_modules/**/*.ts"
       ]
   }
}